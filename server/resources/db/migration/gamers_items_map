CREATE TABLE gamers_items (
    id INT AUTO_INCREMENT PRIMARY KEY,
    item_id INT,
    gamer_id INT,
    status VARCHAR(50),
    x FLOAT,
    y FLOAT,
    FOREIGN KEY (item_id) REFERENCES items(id),
    FOREIGN KEY (gamer_id) REFERENCES gamers(id)
);
CREATE TABLE map (
    height INT,
    width INT,
    content TEXT
);